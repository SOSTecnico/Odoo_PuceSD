<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="racetime.detalle_vacaciones_form_view" model="ir.ui.view">
            <field name="name">racetime.detalle_vacaciones.form</field>
            <field name="model">racetime.detalle_vacaciones</field>
            <field name="arch" type="xml">
                <form string="Detalle Vacaciones">
                    <sheet>
                        <group>
                            <field name="desde"/>
                            <field name="hasta"/>
                            <field name="observacion"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="racetime.detalle_vacaciones_tree_view" model="ir.ui.view">
            <field name="name">racetime.detalle_vacaciones.tree</field>
            <field name="model">racetime.detalle_vacaciones</field>
            <field name="arch" type="xml">
                <tree string="Detalle Vacaciones">
                    <field name="desde"/>
                    <field name="hasta"/>
                    <field name="observacion"/>
                </tree>
            </field>
        </record>

