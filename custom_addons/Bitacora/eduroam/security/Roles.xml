<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record model="ir.module.category" id="eduroam.eduroam_category">
            <field name="name">Eduroam</field>
        </record>

        <record id="eduroam.group_user" model="res.groups">
            <field name="name">Usuario</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="eduroam.eduroam_category"/>
        </record>

        <record id="eduroam.group_admin" model="res.groups">
            <field name="name">Administrador</field>
            <field name="implied_ids" eval="[(4, ref('eduroam.group_user'))]"/>
            <field name="category_id" ref="eduroam.eduroam_category"/>
        </record>

    </data>
</odoo>